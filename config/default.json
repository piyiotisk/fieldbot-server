{
    "jwt": {
        "secret": "default-secret",
        "issuer": "fieldbot-server"
    },
    "winston": {
        "file": {
          "level": "info",
          "filename": "./logs/app.log",
          "handleExceptions": true,
          "json": true,
          "maxsize": 5242880,
          "maxFiles": 5,
          "colorize": false
        },
        "console": {
          "level": "debug",
          "handleExceptions": true,
          "json": false,
          "colorize": true
        }
    }
}